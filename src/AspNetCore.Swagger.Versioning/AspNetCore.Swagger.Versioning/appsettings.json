{
  "SwaggerOptions": {
    "JsonRoute": "swagger/{documentName}/swagger.json",
    "Versions": [
      {
        "Description": "Api v1",
        "UIEndpoint": "v1/swagger.json"
      },
      {
        "Description": "Api v2",
        "UIEndpoint": "v2/swagger.json"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
